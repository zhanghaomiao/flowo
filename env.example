# Application Settings
# External port for Caddy reverse proxy (default: 80)
# Note: Caddy listens on port 80 internally, this maps to external port
# Change this if port 80 is occupied, e.g. PORT=3100
# The service will be accessible at http://localhost:${PORT}

PORT=3100
# DOMAIN=172.16.3.225
DOMAIN=localhost
PROTOCOL=http
SECRET_KEY=your_secret_key
TZ = Asia/Shanghai

# Database Configuration
POSTGRES_DB=flowo_logs
POSTGRES_USER=flowo
POSTGRES_PASSWORD=flowo_password
POSTGRES_HOST=localhost
BACKEND_CORS_ORIGINS=["*"]
# External port for PostgreSQL (default: 5432)
# Note: This is the external port mapping. Container-to-container communication uses port 5432 internally.
# Change this if port 5432 is occupied, e.g. POSTGRES_PORT=5555
POSTGRES_PORT=5555

FLOWO_WORKING_PATH=/home/zhanghm/project/iregene/flowo-working-path
CONTAINER_MOUNT_PATH=/flowo-data
